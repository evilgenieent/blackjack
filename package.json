    "redux": "3.6.0",
    "redux-localstorage": "0.4.1",
    "redux-thunk": "2.2.0"
  },
  "devDependencies": {
    "autoprefixer": "6.7.5",
    "babel-core": "6.23.1",
    "babel-loader": "6.3.2",
    "babel-plugin-lodash": "3.2.11",
    "babel-plugin-transform-es2015-destructuring": "6.23.0",

{
  "name": "blackjack",
  "version": "1.0.0",
  "description": "A blackjack game.",
  "author": "Jonathan Rubins <jonrubins@gmail.com>",
  "scripts": {
    "clean:deps": "rm -rf node_modules",
    "build": "NODE_ENV=production webpack --config webpack.config.prod.js",
    "deploy": "yarn clean:deps && yarn && ./deploy.sh",
    "lint:js": "eslint app buildConfig.js postcss.config.js webpack.config.dev.js webpack.config.prod.js --ext=.js,.jsx",
    "lint:sass": "sass-lint -v -q",
    "lint": "yarn lint:js && yarn lint:sass",
    "start": "git pull && yarn && webpack-dev-server --config webpack.config.dev.js",
    "start:merge": "yarn && webpack-dev-server --config webpack.config.dev.js",
    "start:debug": "yarn && DEBUG=true webpack-dev-server --config webpack.config.dev.js",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "pre-commit": [
    "lint",
    "test"
  ],
  "engines": {
    "node": "6.9.2",
    "npm": "3.10.9"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:jrubins/blackjack.git"
  },
  "license": "UNLICENSED",
  "jest": {
    "cacheDirectory": "./.tmp",
    "resetMocks": true,
    "resetModules": true
  },
  "dependencies": {
    "camelize": "1.0.0",
    "classnames": "2.2.5",
    "core-js": "2.4.1",
    "isomorphic-fetch": "2.2.1",
    "js-cookie": "2.1.3",
    "js-worker-search": "jrubins/js-worker-search#38c4db0",
    "lodash": "4.17.4",
    "normalize.css": "5.0.0",
    "numeral": "2.0.4",
    "object-hash": "1.1.5",
    "react": "15.4.2",
    "react-addons-css-transition-group": "15.4.2",
    "react-addons-shallow-compare": "15.4.2",
    "react-dom": "15.4.2",
    "react-dropzone": "3.9.0",
    "react-redux": "5.0.2",
    "react-router": "3.0.1",
    "react-router-scroll": "0.4.1",
    "redux": "3.6.0",
    "redux-localstorage": "0.4.1",
    "redux-thunk": "2.1.0"
  },
  "devDependencies": {
    "autoprefixer": "6.7.3",
    "babel-core": "6.23.1",
    "babel-loader": "6.2.10",
    "babel-plugin-lodash": "3.2.11",
    "babel-plugin-transform-es2015-destructuring": "6.19.0",
    "babel-plugin-transform-object-rest-spread": "6.20.2",
    "babel-preset-es2015": "6.18.0",
    "babel-preset-react": "6.16.0",
    "css-loader": "0.26.1",
    "dotenv": "4.0.0",
    "eslint": "3.13.1",
    "eslint-plugin-react": "6.9.0",
    "extract-text-webpack-plugin": "2.0.0-rc.3",
    "firebase-tools": "3.2.1",
    "html-webpack-plugin": "2.28.0",
    "jest": "18.1.0",
    "node-sass": "4.5.0",
    "object-sizeof": "1.1.1",
    "postcss-loader": "1.3.0",
    "pre-commit": "1.2.2",
    "react-addons-perf": "15.4.2",
    "react-hot-loader": "3.0.0-beta.6",
    "sass-lint": "1.10.2",
    "sass-loader": "5.0.1",
    "style-loader": "0.13.1",
    "webpack": "2.2.1",
    "webpack-dev-server": "2.3.0",
    "webpack-dotenv-plugin": "2.0.0"
  }
}
